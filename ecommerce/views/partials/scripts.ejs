<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
<script>
    $(document).ready(function () {
        M.AutoInit();
        $('.sidenav').sidenav();
        
        $("#btn1").click(function () {
            var qty1= $("#qty1").val();
            qty1++;
            $("#qty1").val(qty1);
            var total= parseInt($("#totalprice").val());
            total=total +35000;
            $("#totalprice").val(total);
        });
        $("#btn2").click(function () {
            var qty2= $("#qty2").val();
            qty2++;
            $("#qty2").val(qty2);
            var total= parseInt($("#totalprice").val());
            total=total +85000;
            $("#totalprice").val(total);
            
        });
        $("#btn3").click(function () {
            var qty3= $("#qty3").val();
            qty3++;
            $("#qty3").val(qty3);
            var total= parseInt($("#totalprice").val());
            total=total +88000;
            $("#totalprice").val(total);
        });
        
    });
</script>